public struct test
{
    //نمیتونن ارث بری کنن
}

public class animal
{
    public int Id { get; set; }
    public string Name { get; set; }
}
public class cat:animal
{
    public int mustache { get; set; }
}
public class dog : animal
{
    public int speed { get; set; }
}
