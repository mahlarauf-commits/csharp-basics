using System.Net.Http.Headers;
caclutar caclutar = new caclutar();
Console.WriteLine(caclutar.sum(5));
Console.WriteLine(caclutar.Div(a:"10", b:"20"));
Console.WriteLine(caclutar.Sum(new int[] {2,5,8,9,10 }));
public class caclutar
{
   public static int sum(int a , int b=2)
    {
        int c = a + b;
        return c;
    }
    public double Div(string a, string b)
    {
        return double.Parse(a) / double.Parse(b);
    }
    public int Sum(int[] numbers)
    {
        int sum = 0;
        foreach (int i in numbers)
        {
            sum += i;
        }
        return sum;
    }
}
